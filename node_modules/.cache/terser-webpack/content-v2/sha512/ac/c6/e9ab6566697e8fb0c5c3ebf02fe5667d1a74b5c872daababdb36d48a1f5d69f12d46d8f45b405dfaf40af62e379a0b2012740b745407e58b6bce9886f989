енный объем\\n            снятия наличных, безлимитные переводы физлицам и&nbsp;контрагентам.\\n            Торговый эквайринг от&nbsp;1,59% от&nbsp;оборота. Бесплатный\\n            зарплатный проект и&nbsp;ежемесячные начисления до&nbsp;8%\\n            на&nbsp;остаток по&nbsp;счету.\\n            <a tuiLink href=\"#\" onclick=\"return false;\">Подробнее о тарифе</a>\\n        </div>\\n    </tui-radio-labeled>\\n</form>\\n',LESS:\"@import 'taiga-ui-angular-local';\\n\\n.description {\\n    .text-body-xs();\\n    margin-top: @space;\\n    color: var(--tui-text-03);\\n}\\n\"},this.example3={TypeScript:\"import {Component} from '@angular/core';\\nimport {FormControl, FormGroup} from '@angular/forms';\\nimport {changeDetection} from '../../../../../change-detection-strategy';\\nimport {encapsulation} from '../../../../../view-encapsulation';\\n\\n@Component({\\n    selector: 'tui-radio-labeled-example-3',\\n    templateUrl: './index.html',\\n    changeDetection,\\n    encapsulation,\\n})\\nexport class TuiRadioLabeledExample3 {\\n    items = [{name: 'ownership'}, {name: 'lease'}, {name: 'sublease'}];\\n\\n    testForm = new FormGroup({\\n        testValue1: new FormControl(this.items[0]),\\n    });\\n}\\n\",HTML:'<form [formGroup]=\"testForm\">\\n    <tui-radio-labeled\\n        formControlName=\"testValue1\"\\n        class=\"tui-space_bottom-5\"\\n        size=\"l\"\\n        [item]=\"items[0]\"\\n    >\\n        В собственности\\n    </tui-radio-labeled>\\n    <tui-radio-labeled\\n        formControlName=\"testValue1\"\\n        class=\"tui-space_bottom-5\"\\n        size=\"l\"\\n        [item]=\"items[1]\"\\n    >\\n        В аренде\\n    </tui-radio-labeled>\\n    <tui-radio-labeled formControlName=\"testValue1\" size=\"l\" [item]=\"items[2]\">\\n        В субаренде\\n    </tui-radio-labeled>\\n</form>\\n'},this.items=[{name:\"tariff1\"},{name:\"tariff2\"},{name:\"tariff3\"}],this.sizeVariants=[\"m\",\"l\"],this.size=this.sizeVariants[0],this.pseudoDisabled=!1,th